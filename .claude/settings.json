{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "extraKnownMarketplaces": {
    "ornikar-plugins": {
      "source": {
        "source": "github",
        "repo": "ornikar/skills"
      }
    }
  },
  "enabledPlugins": {
    "edu-frontend-plugin@ornikar-plugins": true,
    "common-plugin@ornikar-plugins": true
  },
  "permissions": {
